            <zeroOrMore>
               <ref name="tei_model.graphicLike"/>
            </zeroOrMore>
      
      
            <zeroOrMore>
               <ref name="tei_model.noteLike"/>
            </zeroOrMore>
      
         </group>
         <ref name="tei_att.global.attributes"/>
         <empty/>
      </element>
   </define>
   <define name="tei_glyphName">
